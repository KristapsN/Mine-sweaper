 const gameField = (row: number, col: number) => {
    let shellArray: Array<Array<number | string>> = [];
    for (let i = 0; i < row; i++) {
      let innerArray: Array<number | string> = [];
      for (let j = 0; j < row; j++) {
        innerArray.push('');
      }
      shellArray.push(innerArray);
    }
    return shellArray;
  };
  const randomiser = (scale: number) =>{
    return Math.floor(Math.random()* scale);
  };

  const addBombs = (
    shellArray: Array<Array<number | string>>,
    val: string | number,
    count: number
  ) => {
    let rows = shellArray.length;
    let cols = shellArray[0].length;
    while (count) {
      let y = randomiser(rows);
      let x = randomiser(cols);
      // if (!shellArray[y][x]){
      //   shellArray[y][x] = 
      // }
    }
    return shellArray;
  };